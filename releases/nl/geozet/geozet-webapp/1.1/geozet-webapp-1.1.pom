<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>nl.geozet</groupId>
	<artifactId>geozet-webapp</artifactId>
	<packaging>war</packaging>
	<version>1.1</version>
	<name>GEOZET webapp</name>
	<description>GEOZET viewer webapplicatie ontwikkeld door Geonovum in
		opdracht van ICTU.</description>
	<prerequisites>
		<maven>2.2</maven>
	</prerequisites>
	<distributionManagement>
		<repository>
			<!-- requires a server config in settings.xml: ... <servers> <server> 
				<id>duif-repo</id> <username>gebruiker</username> <password>wachtwoord</password> 
				</server> .... </servers> -->
			<id>duif-repo</id>
			<uniqueVersion>false</uniqueVersion>
			<name>release repository</name>
			<url>svn:http://www.duif.net/geozet/repository/</url>
		</repository>
		<site>
			<id>developer-site</id>
			<name>local developer site</name>
			<url>${site.dir}</url>
		</site>
	</distributionManagement>
	<properties>
		<geotools.version>2.7-M4</geotools.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<geozet.finalname>geozetviewer</geozet.finalname>
	</properties>
	<profiles>
		<profile>
			<id>developer</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<site.dir>file://${java.io.tmpdir}/maven-sites/${project.artifactId}</site.dir>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.mortbay.jetty</groupId>
						<artifactId>maven-jetty-plugin</artifactId>
						<version>6.1.26</version>
						<configuration>
							<scanIntervalSeconds>3</scanIntervalSeconds>
							<stopKey>jetty-stop</stopKey>
							<stopPort>9999</stopPort>
							<connectors>
								<connector implementation="org.mortbay.jetty.nio.SelectChannelConnector">
									<port>8080</port>
									<maxIdleTime>60000</maxIdleTime>
								</connector>
							</connectors>
							<webAppConfig implementation="org.mortbay.jetty.plugin.Jetty6PluginWebAppContext">
								<sessionHandler implementation="org.mortbay.jetty.servlet.SessionHandler">
									<sessionManager implementation="org.mortbay.jetty.servlet.HashSessionManager">
										<usingCookies>false</usingCookies>
									</sessionManager>
								</sessionHandler>
							</webAppConfig>
							<contextPath>/${geozet.finalname}</contextPath>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<!-- om dit profiel te activeren gebruik je -Dhavedot=true of -P havedot, -->
			<id>havedot</id>
			<activation>
				<property>
					<name>havedot</name>
					<value>true</value>
				</property>
			</activation>
			<reporting>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-javadoc-plugin</artifactId>
						<version>2.7</version>
						<configuration>
							<stylesheet>maven</stylesheet>
							<doclet>org.umlgraph.doclet.UmlGraphDoc</doclet>
							<additionalparam>-all -hide java.* -hide javax.* -hide org.* -inferrel -inferreltype -inferdep -useimports</additionalparam>
							<docletArtifact>
								<groupId>org.umlgraph</groupId>
								<artifactId>doclet</artifactId>
								<version>5.3</version>
							</docletArtifact>
							<show>protected</show>
							<useStandardDocletOptions>true</useStandardDocletOptions>
							<links>
								<link>http://logging.apache.org/log4j/1.2/apidocs</link>
								<link>http://download.oracle.com/javase/6/docs/api</link>
								<link>http://download.oracle.com/javaee/5/api</link>
								<link>http://hc.apache.org/httpclient-3.x/apidocs</link>
								<link>http://docs.geotools.org/latest/javadocs</link>
								<!-- deze is er voor de test JavaDocs -->
								<!--<link>${project.url}/apidocs</link> -->
							</links>
							<author>true</author>
							<defaultAuthor />
							<defaultVersion />
							<fixClassComment>true</fixClassComment>
							<fixTags>all</fixTags>
							<javadocVersion>1.6</javadocVersion>
							<encoding>UTF-8</encoding>
							<packagesheader>${project.artifactId}</packagesheader>
							<tags>
								<tag>
									<name>todo</name>
									<placement>a</placement>
									<head>To Do:</head>
								</tag>
								<tag>
									<name>note</name>
									<placement>a</placement>
									<head>Note:</head>
								</tag>
								<tag>
									<name>opt</name>
									<placement>X</placement>
									<head>opt:</head>
								</tag>
							</tags>
							<stylesheet>maven</stylesheet>
						</configuration>
					</plugin>
				</plugins>
			</reporting>
		</profile>
		<profile>
			<id>distribution</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<properties>
				<site.dir>file://${java.io.tmpdir}/maven-sites/${project.artifactId}</site.dir>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-assembly-plugin</artifactId>
						<version>2.2</version>
						<configuration>
							<filters />
							<descriptors>
								<descriptor>src/main/assembly/distfile.xml</descriptor>
							</descriptors>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>release</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<properties>
				<site.dir>file://${java.io.tmpdir}/maven-sites/${project.artifactId}</site.dir>
			</properties>
			<build>
				<plugins>
					<!-- aanpassen compiler opties; optimized en geen debug -->
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-compiler-plugin</artifactId>
						<version>2.3.2</version>
						<configuration>
							<debug>false</debug>
							<optimize>true</optimize>
							<source>1.6</source>
							<target>1.6</target>
							<showDeprecation>false</showDeprecation>
							<showWarnings>false</showWarnings>
							<verbose>false</verbose>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
